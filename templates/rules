# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%RULE_HEAD%

build: build-stamp
build-stamp: 
	dh_testdir%BUILD_PYTHON%
	touch build-stamp

clean: 
	dh_testdir
	dh_testroot
	rm -f build-stamp configure-stamp
	rm -rf build
	find . -name "*.pyc" | xargs rm -f
	rm -f changelog.gz
	dh_clean

install: build
	dh_testdir
	dh_testroot
	dh_clean -k
	dh_installdirs%INSTALL_PYTHON%%INSTALL_ALT%%SUBPACKAGE_INIT%%INSTALL_ELISP%%INSTALL_TEST%


%BINARY_INDEP%

%BINARY_ARCH%

binary: %BINARY_TARGET%
.PHONY: build clean binary %BINARY_TARGET%

